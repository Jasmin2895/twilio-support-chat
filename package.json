    "url": "https://github.com/TwilioDevEd/sdk-starter-node/issues"
  },
  "homepage": "https://github.com/TwilioDevEd/sdk-starter-node#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extend": "^3.0.2",
    "jquery": "^3.5.0",
    "pug": "^2.0.4",
    "twilio": "^3.36.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
